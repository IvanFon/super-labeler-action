{
	"$schema": "http://json.schemastore.org/prettierrc",

	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "none",
	"bracketSpacing": true,
	"arrowParens": "always",
	"insertPragma": true,
	"endOfLine": "crlf"
}
