{
  "name": "super-labeler-action",
  "version": "1.0.0",
  "description": "A superpowered issue and pull request labeler for Github Actions.",
  "main": "src/index.ts",
  "repository": "git@github.com:videndum/super-labeler-action.git",
  "author": "Jonathan Stevens <jonathan@videndum.studio>",
  "license": "GPL-3.0",
  "directories": {
    "lib": "./lib",
    "doc": "./docs",
    "test": "./__tests__"
  },
  "scripts": {
    "all": "npm run build && npm run format && npm run test",
    "build": "tsc && npm run package",
    "format": "prettier --config .prettier.json --write .",
    "format-check": "prettier --config .prettier.json --check .",
    "package": "ncc build --source-map",
    "test": "jest --json --outputFile=result.json --testLocationInResults",
    "dev:run": "source .env node dist/index.js",
    "dev:init": "node bin/copyLocalRun.js"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^2.1.1",
    "@videndum/utilities": "^1.0.0-alpha.2",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/minimatch": "^3.0.3",
    "@vercel/ncc": "^0.24.0",
    "jest": "^26.3.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}
