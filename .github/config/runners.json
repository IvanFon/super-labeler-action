          "requires": 1,
          "conditions": [
            {
              "type": "titleMatches",
              "pattern": "/^bug(\\(.*\\))?:/i"
            },
            {
              "type": "descriptionMatches",
              "pattern": "/(created|new|opened|made)( an| a)? bug/i"
            }
          ]
        },
        "chore": {
          "requires": 1,
          "conditions": [
            {
              "type": "titleMatches",
              "pattern": "/^chore(\\(.*\\))?:/i"
            }
          ]
        },
        "optimisation": {
          "requires": 2,
          "conditions": [
            {
