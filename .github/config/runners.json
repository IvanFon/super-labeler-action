    "pr": {
      "labels": {
        "xs": {
          "requires": 1,
          "conditions": [
            {
              "type": "changesSize",
              "min": 0,
              "max": 10
            }
          ]
        },
        "s": {
          "requires": 1,
          "conditions": [
            {
              "type": "changesSize",
              "min": 10,
              "max": 30
            }
          ]
        },
        "m": {
