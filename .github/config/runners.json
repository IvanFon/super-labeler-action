              "pattern": "/^optimisation(\\(.*\\))?:/i"
            },
            {
              "type": "titleMatches",
              "pattern": "/^maint(\\(.*\\))?:/i"
            },
            {
              "type": "titleMatches",
              "pattern": "/^maintenance(\\(.*\\))?:/i"
            },
            {
              "type": "descriptionMatches",
              "pattern": "/^type:(,| |Style|Refactoring|Revert|Deprecated|Removal)*optimisation/im"
            }
